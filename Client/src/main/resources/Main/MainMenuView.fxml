<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.course.client.controllers.main.MainMenuController">
    <AnchorPane VBox.vgrow="ALWAYS">
      <GridPane layoutX="293.0" layoutY="210.0" prefHeight="272.0" prefWidth="186.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
              <Button fx:id="usersMenuButton" mnemonicParsing="false" onAction="#onUsersButtonClicked" prefHeight="26.0" prefWidth="186.0" text="Пользователи" GridPane.rowIndex="1">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
              <Button fx:id="inventoryMenuButton" defaultButton="true" mnemonicParsing="false" onAction="#onInventoryButtonClicked" prefHeight="26.0" prefWidth="186.0" text="Инвентарь">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
              <Button mnemonicParsing="false" onAction="#onImportButtonClicked" prefHeight="26.0" prefWidth="186.0" text="Импорт данных" GridPane.rowIndex="2">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
              <Button mnemonicParsing="false" onAction="#onExportButtonClicked" prefHeight="26.0" prefWidth="186.0" text="Экспорт данных" GridPane.rowIndex="3">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
              <Button fx:id="logoutButton" cancelButton="true" mnemonicParsing="false" onAction="#onLogoutButtonClicked" prefHeight="26.0" prefWidth="186.0" text="Выход из аккаунта" GridPane.rowIndex="4">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
              <Button fx:id="exitButton" mnemonicParsing="false" onAction="#onExitButtonClicked" prefHeight="26.0" prefWidth="185.0" text="Выход из приложения" GridPane.rowIndex="5">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </GridPane>
        <Label fx:id="userLoginLabel" layoutX="293.0" layoutY="169.0" prefHeight="26.0" prefWidth="475.0" text="Авторизованный пользователь:" />
        <Label layoutX="293.0" layoutY="123.0" prefHeight="26.0" prefWidth="210.0" text="Главное меню">
         <font>
            <Font size="30.0" />
         </font></Label>
    </AnchorPane>
</VBox>
